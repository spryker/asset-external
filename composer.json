{
    "name": "spryker/asset-external",
    "type": "library",
    "description": "AssetExternal module",
    "license": "proprietary",
    "require": {
        "php": ">=7.3",
        "spryker/cms-slot": "^1.0.0",
        "spryker/kernel": "^3.30.0",
        "spryker/store": "^1.15.0",
        "spryker/transfer": "^3.25.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*",
        "spryker/testify": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SprykerTest\\": "tests/SprykerTest/",
            "SprykerTest\\Shared\\AssetExternal\\Helper\\": "tests/SprykerTest/Shared/AssetExternal/_support/Helper/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/ tests/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}

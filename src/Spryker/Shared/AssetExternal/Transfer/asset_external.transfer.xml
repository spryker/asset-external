<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="AssetExternal" strict="true">
        <property name="idAssetExternal" type="int"/>
        <property name="assetUuid" type="string"/>
        <property name="assetName" type="string"/>
        <property name="cmsSlotKey" type="string"/>
        <property name="assetContent" type="string"/>
    </transfer>

    <transfer name="AssetAddedMessage" strict="true">
        <property name="appId" type="string"/>
        <property name="scriptUuid" type="string"/>
        <property name="scriptName" type="string"/>
        <property name="scriptView" type="string"/>
        <property name="slotKey" type="string"/>
        <property name="storeReference" type="string"/>
    </transfer>

    <transfer name="AssetUpdatedMessage" strict="true">
        <property name="appId" type="string"/>
        <property name="scriptUuid" type="string"/>
        <property name="scriptView" type="string"/>
        <property name="slotKey" type="string"/>
        <property name="storeReference" type="string"/>
    </transfer>

    <transfer name="AssetDeletedMessage" strict="true">
        <property name="appId" type="string"/>
        <property name="scriptUuid" type="string"/>
        <property name="storeReference" type="string"/>
    </transfer>

    <transfer name="Store">
        <property name="idStore" type="int"/>
    </transfer>

    <transfer name="CmsSlotCriteria">
        <property name="keys" type="string[]" singular="key"/>
    </transfer>
</transfers>
